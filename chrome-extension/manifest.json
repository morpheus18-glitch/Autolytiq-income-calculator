{
  "manifest_version": 3,
  "name": "Income Calculator Pro",
  "version": "1.0.0",
  "description": "Calculate your projected annual income from YTD earnings. Includes vehicle payment calculator with tax, fees, and trade-in.",
  "author": "Autolytiq",
  "homepage_url": "https://autolytiqs.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Income Calculator Pro"
  },
  "permissions": ["storage"],
  "offline_enabled": true
}
